{"name":"Accelero","type":"feat","img":"systems/pf2e/icons/default-icons/feats.webp","data":{"description":{"value":"<p><strong>Archetype </strong>Chronomancer</p>\n<hr />\n<p><span style=\"font-family: var(--font-primary); font-size: var(--font-size-14);\">Your time control heightens. When you cast Adagio, the target can use this additional action to Strike in addition to its usual effects.</span></p>"},"source":{"value":""},"traits":{"value":["archetype"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.738,"lastMigration":{"datetime":"2022-04-16T18:57:29.349-04:00","version":{"schema":null,"foundry":"9.268","system":"3.8.4.11118"}}},"level":{"value":8},"featType":{"value":"archetype"},"onlyLevel1":false,"maxTakable":1,"actionType":{"value":"passive"},"actionCategory":{"value":""},"actions":{"value":null},"prerequisites":{"value":[{"value":"Chronomancer Dedication"}]},"location":null},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"KPVkkpR21c13yy2y":3},"flags":{"core":{"sourceId":"Item.yU6iB5nTcUZIAppc"}},"_id":"0QdkZyGOt4KRmjOI"}
